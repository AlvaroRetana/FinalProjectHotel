@model IEnumerable<FinalProjectHotel.Models.Admin.RoomModel>

@{
     ViewBag.Title = "AbleRooms";
     Layout = "~/Views/Shared/_Layout.cshtml";
     WebGrid grid = new WebGrid(Model, rowsPerPage: 10);
}


<!-- ============================================================== -->
<!-- Form Habitaciones Listas -->
<!-- ============================================================== -->

<h2>Able Rooms</h2>

    @grid.GetHtml(
        tableStyle: "table",
        mode: WebGridPagerModes.All,
        fillEmptyRows: true,
        firstText: "<< First",
        previousText: "< Previous",
        nextText: "Next >",
        lastText: "Last >>",
        columns: new[]
        {
            grid.Column("ID_Consecutivo" ),
            grid.Column("Nombre"),
            grid.Column("Numero"),
            grid.Column("Descripcion"  ),
            grid.Column("Imagen"),
            grid.Column("Disponibilidad"),
            grid.Column("ID_Precio")

    })
