
@{
    ViewBag.Title = "Precios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


        <!-- ============================================================== -->
        <!-- Form Precios -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="section-block" id="SectionConsecutivo">
                    <h3 class="section-title">Nuevo Precio</h3>

                </div>
                <div class="card">
                    <h5 class="card-header">Nuevo Consecutivo</h5>
                    <div class="card-body">

                        <!-- Tipo:  -->
                        <div class="form-group" id="SectionDescripcion">
                            <label for="ComboBoxTipo">Tipo:</label>
                            <select class="form-control" id="ComboBoxTipo">
                                <option>Normal</option>
                                <option>Deluxe</option>
                                <option>Condominio</option>
                                <option>Licor</option>
                                <option>Bebidas Gaseosas</option>
                                <option>Bebidas Naturales</option>
                                <option>Dulces</option>
                                <option>Helados</option>
                                <option>Flores</option>
                                <option>Tartas</option>
                            </select>
                        </div>
                        <!-- Precio:  -->
                        <div class="form-group" id="SectionNumeroConsecutivo">
                            <label for="InputPrecio" class="col-form-label">Precio:</label>
                            <input id="InputPrecio" type="number" class="form-control" placeholder="0">

                        </div>
                        <!-- Insertar Precio:  -->
                        <div class="form-group">
                            <input class="btn-default" type="button" id="btnSave" value="Guardar" />
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Form Precios  -->
        <!-- ============================================================== -->


        <script src="/Scripts/jqueryLogic-App/jqueryLogin.js"></script>
        <script src="/Content/assets/vendor/jquery/jquery-3.3.1.min.js"></script>
        <script src="/Content/assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
        <script src="/Content/assets/vendor/slimscroll/jquery.slimscroll.js"></script>
        <script src="/Content/assets/libs/js/main-js.js"></script>
        <script src="/Content/assets/vendor/inputmask/js/jquery.inputmask.bundle.js"></script>

        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>






